{
    "typescript.tsdk": "node_modules/typescript/lib",
    "editor.formatOnSave": true,
    "eslint.enable": true,
    "eslint.codeActionsOnSave.mode": "all",
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "always",
        "source.fixAll.sortJSON": "explicit",
        "source.addMissingImports": "explicit"
    },
    "autoimport.autoComplete": true,
    "autoimport.filesToScan": "**/*.{ts,tsx}",
    "workbench.colorCustomizations": {
        "activityBar.activeBackground": "#d5c88b",
        "activityBar.background": "#d5c88b",
        "activityBar.foreground": "#15202b",
        "activityBar.inactiveForeground": "#15202b99",
        "activityBarBadge.background": "#338c7d",
        "activityBarBadge.foreground": "#e7e7e7",
        "commandCenter.border": "#15202b99",
        "sash.hoverBorder": "#d5c88b",
        "statusBar.background": "#c8b665",
        "statusBar.foreground": "#15202b",
        "statusBarItem.hoverBackground": "#b8a342",
        "statusBarItem.remoteBackground": "#c8b665",
        "statusBarItem.remoteForeground": "#15202b",
        "titleBar.activeBackground": "#c8b665",
        "titleBar.activeForeground": "#15202b",
        "titleBar.inactiveBackground": "#c8b66599",
        "titleBar.inactiveForeground": "#15202b99"
    },
    "peacock.color": "#c8b665"
}
